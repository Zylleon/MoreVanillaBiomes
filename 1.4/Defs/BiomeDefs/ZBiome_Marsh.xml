		  <li>
            <terrain>WaterShallow</terrain>
            <min>-0.06</min>
            <max>0.06</max>
          </li>
        </thresholds>
      </li>
	 
	  	<li>										<!-- bs lakes -->
        <perlinFrequency>0.027</perlinFrequency>
		<minFertility>0.15</minFertility>
        <thresholds>
          <li>
            <terrain>SoilRich</terrain>
            <min>0.33</min>
            <max>0.5</max>
          </li>
          <li>
            <terrain>Mud</terrain>
            <min>0.5</min>
