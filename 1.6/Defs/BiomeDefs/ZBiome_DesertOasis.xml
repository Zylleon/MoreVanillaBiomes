		<workerClass>VanillaBiomes.BiomeWorker_DesertOasis</workerClass>

		<settlementSelectionWeight>0.5</settlementSelectionWeight>
		<movementDifficulty>1.5</movementDifficulty>

		<animalDensity>1.5</animalDensity>
		<plantDensity>0.7</plantDensity>
		<wildPlantRegrowDays>17</wildPlantRegrowDays>
		<lakeBeachTerrain>SoilRich</lakeBeachTerrain>
		<riverbankTerrain>SoilRich</riverbankTerrain>
		<riverbankSizeRange>6~8</riverbankSizeRange>

		<maxFishPopulation>660</maxFishPopulation>
